<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.navigation_view">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Navigation_View">
        <activity android:name=".UpdateActivity"></activity>
        <activity android:name=".ProductDetailActivity" />
        <activity android:name=".ProductListActivity" />
        <activity android:name=".CustomerHomeActivity" />
        <activity android:name=".BuyerRegisterActivity" />
        <activity android:name=".SellerRegisterActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".AddProductActivity" />
        <activity android:name=".OrdersActivity" />
        <activity android:name=".CategoriesActivity" />
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>